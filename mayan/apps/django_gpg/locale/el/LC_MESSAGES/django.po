#: settings.py:25
msgid "Keyserver used to query for keys."
msgstr "Διακομιστής κλειδιών που θα χρησιμοποιηθεί για αναζήτηση κλειδιών."

#: views.py:36
#, python-format
msgid "Delete key: %s"
msgstr "Διαγραφή κλειδιού: %s"

#: views.py:53
#, python-format
msgid "Details for key: %s"
msgstr "Λεπτομέρειες κλειδιού: %s"

